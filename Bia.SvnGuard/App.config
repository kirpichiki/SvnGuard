<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="repositoriesConfiguration" type="Bia.SvnGuard.Configuration.RepositoriesConfigurationSection, Bia.SvnGuard"/>
  </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <appSettings>
    <add key="SvnUtilitiesPath" value="C:\"/>
    <add key="StylecopPath" value="C:\"/>
    <add key="RepositoriesPath" value="C:\"/>
  </appSettings>
  <repositoriesConfiguration>
    <repositories>
      <repository name="vs2010" />
      <repository name="_sintek" />
    </repositories>
  </repositoriesConfiguration>
</configuration>